<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.example.pedrobraga.bancofinanca.CompraCRUD"
    tools:showIn="@layout/activity_compra_crud">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.025"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="16dp" />

    <EditText
        android:id="@+id/editText"
        android:layout_width="165dp"
        android:layout_height="16dp"
        android:layout_marginStart="44dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        app:layout_constraintStart_toEndOf="@+id/textView"
        tools:layout_editor_absoluteY="16dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:text="TextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.025"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="62dp" />

    <AutoCompleteTextView
        android:id="@+id/autoCompleteTextView"
        android:layout_width="167dp"
        android:layout_height="15dp"
        android:layout_marginStart="44dp"
        android:text="AutoCompleteTextView"
        app:layout_constraintStart_toEndOf="@+id/textView2"
        tools:layout_editor_absoluteY="62dp" />

    <fragment
        android:id="@+id/fragment"
        android:name="android.support.v7.app.AppCompatDialogFragment"
        android:layout_width="350dp"
        android:layout_height="328dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="108dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="89dp"
        android:layout_height="20dp"
        android:text="Produto"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="124dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Valor"
        tools:layout_editor_absoluteX="235dp"
        tools:layout_editor_absoluteY="124dp" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Quantidade"
        tools:layout_editor_absoluteX="283dp"
        tools:layout_editor_absoluteY="124dp" />

    <AutoCompleteTextView
        android:id="@+id/autoCompleteTextView2"
        android:layout_width="182dp"
        android:layout_height="21dp"
        android:text="AutoCompleteTextView"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="156dp" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="31dp"
        android:layout_height="20dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        tools:layout_editor_absoluteX="237dp"
        tools:layout_editor_absoluteY="156dp" />

    <EditText
        android:id="@+id/editText4"
        android:layout_width="67dp"
        android:layout_height="19dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        tools:layout_editor_absoluteX="287dp"
        tools:layout_editor_absoluteY="156dp" />

    <android.support.v7.widget.RecyclerView
        android:layout_width="315dp"
        android:layout_height="143dp"
        tools:layout_editor_absoluteX="35dp"
        tools:layout_editor_absoluteY="223dp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:text="Total"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.058"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="436dp" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:text="R$"
        app:layout_constraintStart_toEndOf="@+id/textView6"
        tools:layout_editor_absoluteY="436dp" />

    <Button
        android:id="@+id/button2"
        style="@style/Widget.AppCompat.Button.Small"
        android:layout_width="44dp"
        android:layout_height="32dp"
        android:background="@android:drawable/ic_input_add"
        android:backgroundTint="@android:color/holo_blue_light"
        android:backgroundTintMode="multiply"
        android:drawableBottom="@color/colorPrimaryDark"
        android:textAppearance="@style/TextAppearance.AppCompat"
        android:visibility="visible"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="381dp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Categoria"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="190dp" />

    <Spinner
        android:id="@+id/spinner"
        android:layout_width="241dp"
        android:layout_height="20dp"
        tools:layout_editor_absoluteX="108dp"
        tools:layout_editor_absoluteY="189dp" />

</android.support.constraint.ConstraintLayout>
